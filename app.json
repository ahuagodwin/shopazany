{
  "expo": {
    "name": "ShopAzany",
    "slug": "shopazany",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/azany/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/azany/splash3.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chidiahua.shopazany"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/azany/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/azany/splash3.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "08e8ed85-b2c3-45bb-8c57-6734e7e30a0b"
      }
    }
  }
}
